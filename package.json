{
    "version": "0.0.1",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/wurbo/react-mui-carousel.git"
    },
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint && prettier --write .",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "name": "@wurbo/react-mui-carousel",
    "author": "Aaron Merryman",
    "module": "dist/react-mui-carousel.esm.js",
    "size-limit": [
        {
            "path": "dist/react-mui-carousel.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/react-mui-carousel.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@babel/plugin-transform-modules-commonjs": "^7.15.4",
        "@size-limit/preset-small-lib": "^6.0.1",
        "@storybook/addon-essentials": "^6.3.10",
        "@storybook/addon-info": "^5.3.21",
        "@storybook/addon-links": "^6.3.10",
        "@storybook/addons": "^6.3.10",
        "@storybook/react": "^6.3.10",
        "@types/react": "^17.0.27",
        "@types/react-dom": "^17.0.9",
        "@types/use-set-interval": "^1.0.0",
        "babel-loader": "^8.2.2",
        "husky": "^7.0.2",
        "prettier": "^2.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-is": "^17.0.2",
        "size-limit": "^6.0.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "@mui/icons-material": "^5.0.3",
        "@mui/material": "^5.0.3",
        "@mui/styles": "^5.0.1",
        "use-draggable-scroll": "^0.1.0",
        "use-set-interval": "^1.0.4"
    }
